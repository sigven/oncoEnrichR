citHeader("To cite oncoEnrichR in publications use:")

citEntry(
  entry = "Article",
  title = "Comprehensive interrogation of gene lists from genome-scale cancer screens with oncoEnrichR",
  author = personList(
    as.person("Sigve Nakken"),
    as.person("Sveinung Gundersen"),
    as.person("Fabian L. M. Bernal"),
    as.person("Dimitris Polychronopoulos"),
    as.person("Eivind Hovig"),
    as.person("Jørgen Wesche")
  ),
  year = "2023",
  doi = "10.1002/ijc.34666",
  url = "https://doi.org/10.1002/ijc.34666",
  journal = "Int. J. Cancer",
  textVersion  = paste0(
    "Sigve Nakken, Sveinung Gundersen, Fabian L. M. Bernal, ",
    "Dimitris Polychronopoulos, Eivind Hovig, and Jørgen Wesche. ",
    "Comprehensive interrogation of gene lists from genome-scale cancer screens with oncoEnrichR (2023). Int J Cancer. ",
    "https://doi.org/10.1002/ijc.34666"
  )
)
